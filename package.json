{
  "name": "yumemi-front-coding-test",
  "private": true,
  "version": "0.1.0",
  "description": "Yumemi Frontend Coding Test",
  "homepage": "https://batsu0283.github.io/yumemi-front-coding-test/",
  "license": "MIT",
  "author": "batsu0283",
  "repository": {
    "type": "git",
    "url": "https://github.com/batsu0283/yumemi-front-coding-test.git"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome ci ./src",
    "lint:apply": "biome check --write --unsafe ./src",
    "type-check": "tsc --noEmit --project tsconfig.app.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.13.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.4",
    "autoprefixer": "^10.4.20",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "vite": "^5.4.9",
    "vitest": "^2.1.4"
  }
}
